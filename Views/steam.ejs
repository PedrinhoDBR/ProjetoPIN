<!DOCTYPE html>
<html lang="pt-br">
  <head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/css/style.css">
    <title>Document</title>
    <title>foda</title>
  </head>
  <body>
    <table>
      <p style="visibility: hidden;" id="textofodase"><%= result %></p>
      <div class="caixa1" style="margin-top: 3%;">
        <header>
          <h1 style="margin-left: 10%;">Requisito Foda</h1>
          <br>
        </header>
        <p id="demo"></p>
        <img style="padding-left: 30px;" id="imagemid" src="" alt="">
      </div>
    </table>
  </body>
  <script>
    const fodase = document.getElementById("textofodase").textContent
    const imagem = '/imagem'
    const indice = fodase.indexOf(imagem)
    const linkimagem = fodase.substr(indice + imagem.length)
    const requisitos = fodase.substr(0, indice)
    document.getElementById("imagemid").src = linkimagem
    document.getElementById("demo").innerHTML = requisitos
  </script>
</html>